        <StackPanel Orientation="Horizontal" >
            <local:text_ctrl1 DataContext="{Binding DJtextCtrl1}"/>
            <local:text_ctrl1 DataContext="{Binding DJtextCtrl2}"/>
            <local:text_ctrl1 DataContext="{Binding DJtextCtrl3}"/>
